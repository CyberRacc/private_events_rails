<h1 class="text-3xl font-bold"><%= @user.username %>'s Profile</h1>

<p>Email: <%= @user.email %></p>

<h2 class="text-2xl font-bold mt-4">Events Created</h2>
<ul>
  <% @user.created_events.each do |event| %>
    <li>
      <%= link_to event.title, event_path(event), class: "text-blue-500 hover:underline" %>
    </li>
  <% end %>
</ul>
